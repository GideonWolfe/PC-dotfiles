 "java.home": "/usr/lib/jvm/java-12-openjdk/",
 "eslint.trace.server": "verbose",
 "languageserver": {
   "ccls": {
     "command": "ccls",
     "filetypes": [
       "c",
       "cpp",
       "objc",
       "objcpp"],
     "rootPatterns": [
       ".ccls",
       "compile_commands.json",
       ".vim/",
       ".git/",
       ".hg/"],
     "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
     }
    }
   },


}
